      trigger /prod12/rcdas/jrcdas_post_12 == complete
    family mosaic
      task jnldas_forecast
        trigger ../jnldas_prep == complete
      task jnldas_routing
        trigger jnldas_forecast == complete
    endfamily
    family noah
      task jnldas_forecast
        trigger ../jnldas_prep == complete
      task jnldas_routing
        trigger jnldas_forecast == complete
    endfamily
    family sac
